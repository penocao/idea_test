<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.exodus.mapper.UserMapper">

    <select id="insertUser" parameterType="com.exodus.pojo.User">
        insert into user(username,userage) values (#{username},#{userage})
    </select>

    <select id="selectUserAll" resultType="com.exodus.pojo.User">
        select * from user
    </select>
</mapper>